#include <stdio.h>
#include <math.h>

int main()
{
    
    double x1, y1, x2, y2, soma, dist;
   
    scanf("%lf %lf", &x1, &y1);
    scanf("%lf %lf", &x2, &y2);
    
    soma =  ( (pow(x2-x1, 2)) + (pow(y2-y1, 2)) ) ;
    dist = sqrt(soma);
    
    
    printf("%.4lf\n", dist);
        
    
    return 0;
    
}
